  <data name="AppDescription" xml:space="preserve">
    <value>Replaces car generators in the target file with differing car generators from one or more source files. Car generators in the target file are replaced at random, unless a priority list is specified.</value>
  </data>
  <data name="AppName" xml:space="preserve">
