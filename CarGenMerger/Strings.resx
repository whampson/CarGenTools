  </data>
  <data name="AppTitle" xml:space="preserve">
    <value>GTA Car Generator Merger Tool</value>
  </data>
  <data name="AppUsage" xml:space="preserve">
    <value>cgmerge [options] target source [source2 [...]]</value>
  </data>
